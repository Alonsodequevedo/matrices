#include<stdio.h>
#include<stdlib.h>

int main(){
	
	
	int a[5][5]={{1,0,7,1,0},{1,2,4,5,1},{2,4,2,3,2},{3,6,0,4,3},{4,8,2,5,4}};
	int i,j;
	
	for(i=0; i<5; i++)
	{

		printf("%d ",a[i][0]);
		printf("	%d ",a[i][1]);
		printf("	%d ",a[i][2]);
		printf("	%d ",a[i][3]);
		printf("	%d \n",a[i][4]);
	}
	return 0;
}
