#include <stdio.h>

int main() {
    int numeros[5];
    int numerosDuplicados[5];
	int i;
    
    
    printf("Ingresa 5 numeros enteros:\n");
    for (i = 0; i < 5; i++) 
	{
        printf("Numero %d: ", i + 1);
        scanf("%d", &numeros[i]);
    }


    for (i = 0; i < 5; i++) 
	{
        numerosDuplicados[i] = numeros[i] * 2;
    }

    
    printf("Valores duplicados:\n");
    for (i = 0; i < 5; i++) 
	{
        printf("%d ", numerosDuplicados[i]);
    }
    printf("\n");

    return 0;
}
